const {
  int max = 100;
}

main() bool {
  local {
    int n;
  }

  write("introduza um nÃºmero entre 0 e ", max);

  n = read();

  if (n > max or n < 0) {
    write("Erro!!!");
  } else {
    write(n, "!=", fact(n));
    main = true;
  }
}

fact (int n) int {
  local {
    int i;
  }

  fact = 1;

  for (i,2,n,1) {
    fact *= i;
  }
}
