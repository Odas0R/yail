structs {
  array3D {float x, y, z;};
}

main() bool {
  local {array3D v1, v2;}
  write("Introduza dois vetores");
  v1 = read_all();
  v2 = read_all();
  write("O produto escalar dos vetores v1 e v2 Ã©: ", dotProduct(v1, v2));
  main = true;
}

dotProduct(array3D v1, array3D v2) float {
  local {float product;}
  product = v1.x * v2.x + v1.y * v2.y + v1.z * v2.z;
  dotProduct = product;
}
